                    <property
                      dataType="System.String"
                      description="The SQL command to be executed."
                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">WITH Dates AS(
SELECT [DateID]
      ,[Date]
      ,[Year]
      ,[QuarterID]
      ,[QuarterDesc]
      ,[MonthID]
      ,[Month]
  FROM [MatavDW].[dbo].[DW_DIM_Dates])


SELECT D.Date AS Period
      ,I.[No]
	   ,CONVERT(VARCHAR, I.[No]) + '_' + CONVERT(VARCHAR,I.[Snif]) AS Customer_Key
      ,I.[FromDate]
      ,ISNULL(I.[ToDate],EOMONTH(GETDATE(), 0)) AS ToDate
	  ,I.[Detail]
      ,I.[Teur]
      ,I.[KodHos]
      ,I.[IspuzID]
      ,I.[IsUpdateAfterHen]
      ,I.[IsBlockSaot]
      ,I.[InsertDate]
      ,I.[IUserID]
      ,I.[UpdateDate]
      ,I.[UUserID]
      ,I.[Snif]
	  ,I.[DW_UploadDate]
	  ,CASE WHEN DATEDIFF(MM,I.[FromDate],ISNULL(I.[ToDate],EOMONTH(GETDATE(), 0)))=0 
       THEN DATEDIFF(DD,I.[FromDate],ISNULL(I.[ToDate],EOMONTH(GETDATE(), 0)))+1
       ELSE 
              CASE WHEN  DATEDIFF(MM,D.Date,I.[FromDate])=0 AND DATEDIFF(MM,D.Date,ISNULL(I.[ToDate],EOMONTH(GETDATE(), 0)))&lt;&gt;0
                   THEN  DATEDIFF(DD,I.[FromDate],EOMONTH(I.[FromDate], 0))+1
                   ELSE
                        CASE WHEN DATEDIFF(MM,D.Date,ISNULL(I.[ToDate],EOMONTH(GETDATE(), 0)))=0
                        THEN DATEDIFF(DD,D.Date,ISNULL(I.[ToDate],EOMONTH(GETDATE(), 0)))+1
                        ELSE 
                             CASE WHEN DATEDIFF(MM,D.Date,I.[FromDate])&lt;&gt;0  AND DATEDIFF(MM,D.Date,ISNULL(I.[ToDate],EOMONTH(GETDATE(), 0)))&lt;&gt;0 AND Date BETWEEN I.[FromDate] AND ISNULL(I.[ToDate],EOMONTH(GETDATE(), 0))
                              THEN DATEDIFF(DD,D.Date,EOMONTH(D.Date,0))+1
                              END
                         END
              END
       END AS AbsenceDays
FROM Dates as D
Inner Join [MatavOperational].[dbo].[ODS_ispuz] I 
ON  DATEADD(DD, 1, EOMONTH(I.[FromDate], -1))&lt;= D.Date And D.Date&lt;=DATEADD(DD, 1, EOMONTH(ISNULL(I.[ToDate],EOMONTH(GETDATE(), 0)), -1))
where Day(Date)=1</property>

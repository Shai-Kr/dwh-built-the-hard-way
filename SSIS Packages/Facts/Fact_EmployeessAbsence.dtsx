                    <property
                      dataType="System.String"
                      description="The SQL command to be executed."
                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">WITH Dates AS(
SELECT [DateID]
      ,[Date]
      ,[Year]
      ,[QuarterID]
      ,[QuarterDesc]
      ,[MonthID]
      ,[Month]
  FROM [MatavDW].[dbo].[DW_DIM_Dates])


SELECT D.Date AS Period
	  ,M.[MetNo]
	  ,CONVERT(VARCHAR, M.[MetNo]) + '_' + CONVERT(VARCHAR,M.[Snif]) AS Employee_Key
      ,M.[FromDate]
      ,ISNULL(M.[ToDate],EOMONTH(GETDATE(), 0)) AS ToDate
      ,M.[AbsenceKod]
      ,M.[Detail]
      ,M.[InsertDate]
      ,M.[IUserID]
      ,M.[UpdateDate]
      ,M.[UUserID]
      ,M.[SemelIllness]
      ,M.[Snif]
      ,M.[DW_UploadDate]
	  ,CASE WHEN DATEDIFF(MM,M.[FromDate],ISNULL(M.[ToDate],EOMONTH(GETDATE(), 0)))=0 
       THEN DATEDIFF(DD,M.[FromDate],ISNULL(M.[ToDate],EOMONTH(GETDATE(), 0)))+1
       ELSE 
              CASE WHEN  DATEDIFF(MM,D.Date,M.[FromDate])=0 AND DATEDIFF(MM,D.Date,ISNULL(M.[ToDate],EOMONTH(GETDATE(), 0)))&lt;&gt;0
                   THEN  DATEDIFF(DD,M.[FromDate],EOMONTH(M.[FromDate], 0))+1
                   ELSE
                        CASE WHEN DATEDIFF(MM,D.Date,ISNULL(M.[ToDate],EOMONTH(GETDATE(), 0)))=0
                        THEN DATEDIFF(DD,D.Date,ISNULL(M.[ToDate],EOMONTH(GETDATE(), 0)))+1
                        ELSE 
                             CASE WHEN DATEDIFF(MM,D.Date,M.[FromDate])&lt;&gt;0  AND DATEDIFF(MM,D.Date,ISNULL(M.[ToDate],EOMONTH(GETDATE(), 0)))&lt;&gt;0 AND Date BETWEEN M.[FromDate] AND ISNULL(M.[ToDate],EOMONTH(GETDATE(), 0))
                              THEN DATEDIFF(DD,D.Date,EOMONTH(D.Date,0))+1
                              END
                         END
              END
       END AS AbsenceDays
FROM Dates as D
Inner Join [MatavOperational].[dbo].[ODS_metabsence] M 
ON  DATEADD(DD, 1, EOMONTH(M.[FromDate], -1))&lt;= D.Date And D.Date&lt;=DATEADD(DD, 1, EOMONTH(ISNULL(M.[ToDate],EOMONTH(GETDATE(), 0)), -1))
where Day(Date)=1</property>
